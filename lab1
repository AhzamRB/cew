#include <stdio.h>

void binaryConverter(int userInt)
 {
     
    if (userInt == 0) {
         printf("0");
         return;
         }
    
    int binary[32];
    int index = 0;
    
    while (userInt > 0 ) {
        
        binary[index] = userInt % 2;
        index++;
        userInt = userInt / 2;
        }
    
    for(int i = index - 1; i >= 0; i--) {
        printf("%d", binary[i]);
        }
    
}

int main(int argc, char **argv)
{   
    binaryConverter(112);
    
}
